<openerp>
	<data>

		<record id="tbvip_action_tbvip_demand" model="ir.actions.act_window">
			<field name="name">Demand</field>
			<field name="res_model">tbvip.demand</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="domain">[('demand_type', '=', 'interbranch')]</field>
		</record>

		<record id="tbvip_action_tbvip_demanded_products" model="ir.actions.act_window">
			<field name="name">Demanded Products</field>
			<field name="res_model">tbvip.demand.line</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="domain">[('state', '!=', 'ready_for_transfer')]</field>
		</record>

		<record id="tbvip_action_tbvip_demand_management_guest" model="ir.actions.act_window">
			<field name="name">Demand Management Guest</field>
			<field name="res_model">tbvip.demand</field>
			<field name="view_type">form</field>
			<field name="view_mode">tree,form</field>
			<field name="domain">[('demand_type', '=', 'different_management')]</field>
			<field name="context">{'default_demand_type': 'different_management'}</field>
		</record>

		<record id="tbvip_action_tbvip_respond_demands" model="ir.actions.act_window">
			<field name="name">Respond Demands</field>
			<field name="res_model">tbvip.demand.respond.memory</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		</record>

		<act_window name="Respond Demands"
				res_model="tbvip.demand.respond.memory"
				src_model="tbvip.demand.line"
				view_mode="form"
				target="new"
				multi="True"
				key2="client_action_multi"
				id="tbvip_action_demand_product_respond"/>

		<menuitem sequence="1" id="tbvip_menu_tbvip" name="Demand" parent="stock.menu_stock_root"/>
			<menuitem sequence="11" id="tbvip_menu_tbvip_demand"
					parent="tbvip_menu_tbvip" action="tbvip_action_tbvip_demand"/>
			<menuitem sequence="12" id="tbvip_menu_tbvip_demanded_products"
				parent="tbvip_menu_tbvip" action="tbvip_action_tbvip_demanded_products"/>
			<menuitem sequence="13" id="tbvip_menu_tbvip_demand_management_guest"
				parent="tbvip_menu_tbvip" action="tbvip_action_tbvip_demand_management_guest"/>
	</data>
</openerp>